{
	"name": "gruz0/remove-noreferrer",
	"keywords": [ "phpunit" ],
	"require": {
		"php": ">=7.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
		"10up/wp_mock": "^0.4.2"
	},
	"scripts": {
		"test": [
			"vendor/bin/phpunit --no-coverage -c tests/phpunit/phpunit.xml"
		],
		"coverage": [
			"vendor/bin/phpunit -c tests/phpunit/phpunit.xml"
		],
		"coverage-local": [
			"vendor/bin/phpunit -c tests/phpunit/phpunit.xml --coverage-html ./build/coverage-html"
		]
	}
}
