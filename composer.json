{
	"name": "gruz0/remove-noreferrer",
	"keywords": [ "phpunit" ],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.0 || ^6.5 || ^7.0 || ^8.0",
		"squizlabs/php_codesniffer": "^3.5",
		"wp-coding-standards/wpcs": "^2.3",
		"phpcompatibility/php-compatibility": "*",
		"phpcompatibility/phpcompatibility-wp": "*",
		"phpcompatibility/phpcompatibility-paragonie": "*"
	},
	"prefer-stable" : true,
	"scripts": {
		"test": [
			"vendor/bin/phpunit -v --debug --colors=always --no-coverage -c tests/phpunit/phpunit.xml"
		],
		"coverage": [
			"vendor/bin/phpunit -v --debug --colors=always -c tests/phpunit/phpunit.xml"
		]
	}
}
